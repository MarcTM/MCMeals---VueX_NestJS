n((response) => {\n              console.log(response);\n              this.$router.push({ name: \"Login\"});\n            })\n            .catch((response) => {console.log(response.data)})\n        }\n    },\n  }\n</script>\n\n\n<style scoped>\n  .register {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    padding: 20px;\n  }\n\n  .register h1 {\n    margin-top: 40px;\n    width: 80%;\n    padding-bottom: 20px;\n    border-bottom: 1px solid grey;\n  }\n\n  .register-form {\n    margin-top: 40px;\n    display: flex;\n    flex-direction: column;\n    width: 80%;\n  }\n\n  form label {\n    text-align: left;\n    display: block;\n    margin-bottom: 10px;\n    font-weight: bold;\n    text-transform: uppercase;\n    font-size: 0.8em;\n  }\n\n  form input {\n    padding-left: 10px;\n    width: 40%;\n    height: 50px;\n    margin-bottom: 40px;\n    border: 1.5px solid grey;\n    border-radius: 0;\n  }\n\n  .register-button {\n    cursor: pointer;\n    width: 20%;\n    height: 50px;\n    color: white;\n    background-color: grey;\n    border: 1px solid grey;\n  }\n\n  .register-button:hover {\n    transition: 0.4s;\n    background-color: #f7432e;\n    border: 1px solid #f7432e;\n  }\n</style>"]}]}