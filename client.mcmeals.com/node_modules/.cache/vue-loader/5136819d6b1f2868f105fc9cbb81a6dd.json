on v-if=\"most_visited\" class=\"popular\">\n      <span class=\"popular-title\">MOST POPULAR</span>\n      <section class=\"popular-products\">\n        <ProductPreview v-for=\"product in most_visited\" v-bind:product=\"product\" />\n      </section>\n    </section>\n\n  </section>\n</template>\n\n\n\n<script>\nimport { defineComponent } from 'vue';\nimport ProductPreview from '@/components/ProductPreview.vue';\nimport {\n  GET_CATEGORIES,\n  GET_MOST_VISITED\n} from \"@/store/actions.type\";\nimport { mapGetters } from \"vuex\";\n\nexport default defineComponent({\n  name: 'Home',\n\n  data() {\n    return {\n      hoverCategory: false,\n    };\n  },\n\n  components: {\n    ProductPreview\n  },\n\n  mounted() {\n      this.getCategories();\n      this.getMostVisited();\n  },\n\n  computed: {\n      ...mapGetters([\"categories\", \"most_visited\"]),\n  },\n\n  methods: {\n      getCategories() {\n          this.$store.dispatch(GET_CATEGORIES);\n      },\n\n      getMostVisited() {\n        this.$store.dispatch(GET_MOST_VISITED);\n      },\n\n      getCategory(slug) {\n        this.$router.push({ name: \"Category\", params: { slug: slug }});\n      }\n  },\n});\n</script>\n\n<style src=\"./Home.css\" scoped />"]}]}