button @click=\"deleteProduct(selected)\" class=\"button\">DELETE</button>\n        </section>\n      </section>\n    </section>\n\n  </section>\n</template>\n\n\n<script>\nimport {\n  GET_USERS,\n  DELETE_USER\n} from \"@/store/actions.type\";\nimport { mapGetters } from \"vuex\"\n\nexport default {\n  name: 'Admin-Users',\n\n  data() {\n    return {\n      openModal: false,\n      selected: false,\n    }\n  },\n\n  mounted() {\n      this.getUsers()\n  },\n\n  computed: {\n      ...mapGetters([\"users\"]),\n  },\n\n  methods: {\n      getUsers() {\n          this.$store.dispatch(GET_USERS)\n      },\n\n      selectUser(id) {\n        this.openModal = true;\n        this.selected = id;\n      },\n\n      updateProduct(id) {\n      },\n\n      deleteProduct(id) {\n        this.$store.dispatch(DELETE_USER)\n      },\n  },\n};\n</script>\n\n<style src=\"./Users.css\" scoped />"]}]}