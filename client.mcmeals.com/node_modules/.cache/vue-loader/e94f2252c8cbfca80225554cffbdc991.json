odal = false\" class=\"close\">&times;</span>\n        <section v-if=\"selected\">\n          <button @click=\"updateCategory(selected)\" class=\"button\">UPDATE</button>\n          <button @click=\"deleteCategory(selected)\" class=\"button\">DELETE</button>\n        </section>\n      </section>\n    </section>\n\n  </section>\n</template>\n\n\n<script>\nimport { GET_CATEGORIES } from \"@/store/actions.type\";\nimport { mapGetters } from \"vuex\"\n\nexport default {\n  name: 'Admin-Categories',\n\n  data() {\n    return {\n      openModal: false,\n      selected: false,\n    }\n  },\n\n  mounted() {\n      this.getCategories()\n  },\n\n  computed: {\n      ...mapGetters([\"categories\"]),\n  },\n\n  methods: {\n      getCategories() {\n          this.$store.dispatch(GET_CATEGORIES)\n      },\n\n      selectCategory(id) {\n        this.openModal = true;\n        this.selected = id;\n      },\n\n      updateCategory(id) {\n      },\n\n      deleteCategory(id) {\n        this.$store.dispatch(DELETE_CATEGORY)\n      },\n  },\n};\n</script>\n\n<style src=\"./Categories.css\" scoped />"]}]}