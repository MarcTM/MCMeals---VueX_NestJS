r(name, surname, email, password, rpassword) {\n          if (password === rpassword) {\n            this.$store.dispatch(REGISTER, { name, surname, email, password })\n              .then((response) => {\n                this.$router.push({ name: \"Login\"})\n              })\n              .catch((error) => {\n                console.error(error);\n              })\n          }\n        }\n    },\n  }\n</script>\n\n<style src=\"./Register.css\" scoped />"]}]}