und-color: var(--color-background-primary);\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    width: 100%;\n    \n    box-sizing: border-box;\n    padding: 20px;\n  }\n\n  .main-header a {\n    color: white;\n    text-decoration: none;\n    margin: 0px 20px;\n    text-transform: uppercase;\n    font-weight: bold;\n  }\n\n  .main-header-top a {\n    font-size: 14px;\n  }\n\n  .main-header-bottom a {\n    font-size: 18px;\n  }\n\n  .main-header a:hover {\n    transition: 0.2s;\n    color: var(--color-hover-light);\n  }\n\n  .main-header-top {\n    width: 100%;\n    text-align: right;\n    color: white;\n  }\n\n  .main-header-logo {\n    width: 100%;\n    text-align: center;\n  }\n\n  .main-header-logo img {\n    width: 110px;\n  }\n\n  .main-header-bottom {\n    width: auto;\n    margin: 26px 0px 16px 0px;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n  }\n</style>"]}]}