ail, password })\n            .then((response) => {\n              if (response.user) {\n                if (response.user.role === \"admin\") {\n                  this.$router.push({ name: \"Admin-Users\"})\n                } else {\n                  this.$router.push({ name: \"Home\"})\n                }\n              }\n            })\n            .catch((error) => {\n              console.error(error)\n            })\n        }\n    },\n  }\n</script>\n\n<style src=\"./Login.css\" scoped />"]}]}