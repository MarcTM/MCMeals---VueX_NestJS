CD;AACF,GA9BgB;AAiCjB;AACA,EAAA,MAlCiB,kBAkCV,QAlCU,EAkCQ,IAlCR,EAkCsB,MAlCtB,EAkCiC;AAChD,WAAO,KAAK,CAAC,GAAN,WAAa,OAAb,cAAwB,QAAxB,cAAoC,IAApC,GAA4C,MAA5C,CAAP;AACD,GApCgB;AAuCjB;AACA,EAAA,GAxCiB,eAwCb,QAxCa,EAwCK,MAxCL,EAwCgB;AAC/B,WAAO,KAAK,CAAC,GAAN,WAAa,OAAb,cAAwB,QAAxB,GAAoC,MAApC,CAAP;AACD,GA1CgB;AA6CjB;AA7CiB,6BA8CV,QA9CU,EA8CqB;AAAA,QAAb,OAAa,uEAAL,KAAK;;AACpC,QAAI,OAAJ,EAAa;AACX,aAAO,KAAK,UAAL,WAAgB,OAAhB,cAA2B,QAA3B,GAAuC;AAAC,QAAA,OAAO,EAAP;AAAD,OAAvC,CAAP;AACD,KAFD,MAEO;AACL,aAAO,KAAK,UAAL,WAAgB,OAAhB,cAA2B,QAA3B,EAAP;AACD;AACF;AApDgB,CAAnB;AAuDA,eAAe,UAAf","sourcesContent":["import axios, { AxiosRequestConfig } from \"axios\";\nimport JwtService from \"@/common/jwt.service\";\nimport { API_URL } from \"@/common/config\";\n\n\nconst headers = {\n  \"Authorization\": `Token ${JwtService.getToken()}`,\n}\n\n\nconst ApiService = {\n  // Query\n  query(resource: string, params: any) {\n    return axios.get(`${API_URL}/${resource}?${params}`).catch((error) => {\n      throw new Error(`[RWV] ApiService ${error}`);\n    });\n  },\n\n\n  // Get\n  get(resource: string, context = false) {\n    if (context) {\n      return axios.get(`${API_URL}/${resource}`, {headers}).catch((error) => {\n        throw new Error(`[RWV] ApiService ${error}`)\n      });\n    } else {\n      return axios.get(`${API_URL}/${resource}`).catch((error) => {\n        throw new Error(`[RWV] ApiService ${error}`)\n      });\n    }\n  },\n\n\n  // Post\n  post(resource: string, params: any, context = false) {\n    if (context) {\n      return axios.post(`${API_URL}/${resource}`, params, {headers});\n    } else {\n      return axios.post(`${API_URL}/${resource}`, params);\n    }\n  },\n\n\n  // Update\n  update(resource: string, slug: string, params: any) {\n    return axios.put(`${API_URL}/${resource}/${slug}`, params);\n  },\n\n\n  // Put\n  put(resource: string, params: any) {\n    return axios.put(`${API_URL}/${resource}`, params);\n  },\n\n\n  // Delete\n  delete(resource: string, context=false) {\n    if (context) {\n      return axios.delete(`${API_URL}/${resource}`, {headers})\n    } else {\n      return axios.delete(`${API_URL}/${resource}`);\n    }\n  }\n};\n\nexport default ApiService;"],"sourceRoot":""}]}