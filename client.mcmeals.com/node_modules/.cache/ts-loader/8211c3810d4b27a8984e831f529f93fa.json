 });\n  },\n};\n\n\nconst mutations = {\n  // Set errors\n  [SET_ERROR](state: any, error: any) {\n    state.errors = error;\n  },\n\n\n  // Set categories\n  [SET_CATEGORIES](state: any, categories: any) {\n    state.errors = null;\n    state.categories = categories;\n  },\n\n\n  // Set category\n  [SET_CATEGORY](state: any, category: any) {\n    state.errors = null;\n    state.category = category;\n  },\n};\n\n\nexport default {\n  state,\n  actions,\n  mutations,\n  getters\n};"],"sourceRoot":""}]}