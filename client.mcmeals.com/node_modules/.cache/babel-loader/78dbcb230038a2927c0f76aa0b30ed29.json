D;AAAA;AAA5D,MAA4D,CAAA,SAAA,CAA5D,CACQ,CAHV,CAIQ,CANV,CAmBQ,EAXR,YAAA,CAAgF,MAAhF,EAAA,UAAA,EAAoC,aAAQ,gBAAA,CAAE,OAAO,CAAC,OAAR,CAAgB,MAAlB,CAAR,GAAkC,KAAtE,EAAyE;AAAA;AAAzE,KAWQ,EAVR,YAAA,CAAuE,MAAvE,EAAA,WAAA,EAAkC,WAAM,gBAAA,CAAE,OAAO,CAAC,OAAR,CAAgB,IAAlB,CAAxC,EAA8D;AAAA;AAA9D,KAUQ,EARR,YAAA,CAOU,SAPV,EAAA,WAAA,EAOU,CANR,WAMQ,EALR,YAAA,CAIU,SAJV,EAAA,WAAA,EAIU,CAHR,YAAA,CAA2D,QAA3D,EAA2D;AAAlD,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,QAAA,CAAA,IAAA,CAAK,OAAL,EAAc,OAAO,CAAC,QAAtB,CAAF;AAAA;AAA6C,KAA3D,EAAiD,GAAjD,EAAkD;AAAA;AAAlD,MAAkD,CAAA,SAAA,CAAlD,CAGQ,EAFR,YAAA,CAAiC,MAAjC,EAAiC,IAAjC,EAAiC,gBAAA,CAAzB,OAAO,CAAC,QAAiB,CAAjC,EAAwB;AAAA;AAAxB,KAEQ,EADR,YAAA,CAA2D,QAA3D,EAA2D;AAAlD,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,QAAA,CAAA,IAAA,CAAK,OAAL,EAAc,OAAO,CAAC,QAAtB,CAAF;AAAA;AAA6C,KAA3D,EAAiD,GAAjD,EAAkD;AAAA;AAAlD,MAAkD,CAAA,SAAA,CAAlD,CACQ,CAJV,CAKQ,CAPV,CAQQ,CApBV,CAsBQ,CAzBV,C;GAyBU,CAzBV,E;;AAAA,GA0BQ,EA3BV,CAsCQ,EATR,YAAA,CAQU,SARV,EAAA,WAAA,EAQU,CAPR,YAAA,CAGU,SAHV,EAAA,WAAA,EAGU,CAFR,WAEQ,EADR,YAAA,CAA0B,MAA1B,EAA0B,IAA1B,EAAM,MAAC,gBAAA,CAAE,IAAA,CAAA,QAAF,CAAP,EAAiB;AAAA;AAAjB,GACQ,CAHV,CAOQ,EAFR,WAEQ,EADR,YAAA,CAA4E,MAA5E,EAA4E;AAArE,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,QAAA,EAAF;AAAA,KAAA,CAAgE;AAAlD,aAAM;AAA4C,GAA5E,EAAoD,mBAApD,CACQ,CARV,CASQ,CAvCV,C,mBAyCA,YAAA,CAGU,SAHV,EAAA,WAAA,EAGU,CAFR,WAEQ,EADR,YAAA,CAAsD,QAAtD,EAAsD;AAA7C,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,QAAA,EAAF;AAAA,KAAA;AAAwC,GAAtD,EAA4B,mBAA5B,CACQ,CAHV,C,CAKQ,CAlDV,C","sourcesContent":["<template>\n  <section class=\"cart\">\n\n    <h1 class=\"cart-header\">SHOPPING CART</h1>\n\n    <section class=\"cart-section\" v-if=\"cart && cart.length > 0\">\n      <section class=\"cart-products\">\n        <section v-for=\"article in cart\" class=\"article\">\n          <img @click=\"details(article.product.slug)\" class=\"article-image\" :src=\"article.product.image\" />\n\n          <section class=\"article-info\">\n            <section class=\"article-name-price\">\n              <span class=\"article-name\" @click=\"details(article.product.slug)\">{{article.product.name}}</span>\n              <section class=\"article-price\">\n                <span>€{{article.product.price}}</span>\n                <span @click=\"removeFromCart(article)\" class=\"remove-cart\">x</span>\n              </section>\n            </section>\n\n            <span class=\"article-weight italic\">Weight: {{article.product.weight}} KG</span>\n            <span class=\"article-slug italic\">Code: {{article.product.slug}}</span>\n\n            <section class=\"quantity-section\">\n              <span>QTY:</span>\n              <section class=\"quantity\">\n                <button @click=\"less(article, article.quantity)\">-</button>\n                <span>{{article.quantity}}</span>\n                <button @click=\"more(article, article.quantity)\">+</button>\n              </section>\n            </section>\n          </section>\n          \n        </section>\n      </section>\n\n      <section class=\"cart-subtotal\">\n        <section class=\"subtotal\">\n          <span>Subtotal</span>\n          <span>€{{subtotal}}</span> \n        </section>\n\n        <button class=\"checkout\">CHECKOUT</button>\n        <span @click=\"goToShop()\" class=\"continue-shopping\">CONTINUE SHOPPING</span>\n      </section>\n    </section>\n\n    <section v-else class=\"no-cart\">\n      <span>There are no products in your cart</span>\n      <button @click=\"goToShop()\">CONTINUE SHOPPING</button>\n    </section>\n  \n  </section>\n</template>\n\n\n\n<script>\nimport {\n    GET_CART,\n    UPDATE_CART,\n    REMOVE_CART,\n} from \"@/store/actions.type\";\nimport { mapGetters } from 'vuex';\n\nexport default {\n  name: \"Cart\",\n\n  mounted() {\n      this.get_cart()\n  },\n\n  computed: {\n      ...mapGetters([\"cart\", \"subtotal\"]),\n  },\n\n  methods: {\n      get_cart() {\n          this.$store.dispatch(GET_CART);\n      },\n\n      goToShop() {\n        this.$router.push({ name: \"Shop\"})\n      },\n\n      details(slug) {\n        this.$router.push({ name: \"Product\", params: { slug }});\n      },\n      \n      less(article, quantity) {\n        if (quantity > 0) {\n          quantity -= 1;\n          this.$store.dispatch(UPDATE_CART, { article, quantity });\n        }\n      },\n\n      more(article, quantity) {\n        quantity += 1;\n        this.$store.dispatch(UPDATE_CART, { article, quantity });\n      },\n\n      removeFromCart(article) {\n        this.$store.dispatch(REMOVE_CART, article);\n      }\n  },\n}\n</script>\n\n<style src=\"./Cart.css\" scoped />"],"sourceRoot":""}]}