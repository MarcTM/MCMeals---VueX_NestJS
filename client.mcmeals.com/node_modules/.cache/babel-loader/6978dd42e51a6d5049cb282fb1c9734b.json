roduct.name}}</h4>\n        <img v-bind:src=\"product.image\" alt=\"product image\" />\n      </section>\n    </section>\n\n\n    <section class=\"modal\" v-bind:class=\"{ 'showed': openModal }\">\n      <section class=\"modal-content\">\n        <span @click=\"openModal = false\" class=\"close\">&times;</span>\n        <section v-if=\"selected\">\n          <button @click=\"updateProduct(selected)\" class=\"button\">UPDATE</button>\n          <button @click=\"deleteProduct(selected)\" class=\"button\">DELETE</button>\n        </section>\n      </section>\n    </section>\n\n  </section>\n</template>\n\n\n<script>\nimport {\n  GET_PRODUCTS,\n  DELETE_PRODUCT\n} from \"@/store/actions.type\";\nimport { mapGetters } from \"vuex\"\n\nexport default {\n  name: 'Admin-Products',\n\n  data() {\n    return {\n      openModal: false,\n      selected: false,\n    }\n  },\n\n  mounted() {\n      this.getProducts()\n  },\n\n  computed: {\n      ...mapGetters([\"products\"]),\n  },\n\n  methods: {\n      getProducts() {\n          this.$store.dispatch(GET_PRODUCTS)\n      },\n\n      selectProduct(id) {\n        this.openModal = true;\n        this.selected = id;\n      },\n\n      updateProduct(id) {\n\n      },\n\n      deleteProduct(id) {\n        this.$store.dispatch(DELETE_PRODUCT)\n      },\n  },\n};\n</script>\n\n<style src=\"./Products.css\" scoped />"],"sourceRoot":""}]}