   <span @click=\"openModal = false\" class=\"close\">&times;</span>\n        <section v-if=\"selected\">\n          <button @click=\"updateProduct(selected)\" class=\"button\">UPDATE</button>\n          <button @click=\"deleteProduct(selected)\" class=\"button\">DELETE</button>\n        </section>\n      </section>\n    </section>\n\n  </section>\n</template>\n\n\n\n<script>\nimport { defineComponent } from 'vue';\nimport {\n  GET_PRODUCTS,\n  DELETE_PRODUCT\n} from \"@/store/actions.type\";\nimport { mapGetters } from \"vuex\"\n\nexport default defineComponent({\n  name: 'Products',\n\n  data() {\n    return {\n      openModal: false,\n      selected: false,\n    }\n  },\n\n  mounted() {\n      this.getProducts()\n  },\n\n  computed: {\n      ...mapGetters([\"products\"]),\n  },\n\n  methods: {\n      getProducts() {\n          this.$store.dispatch(GET_PRODUCTS)\n      },\n\n      selectProduct(id) {\n        this.openModal = true;\n        this.selected = id;\n      },\n\n      updateProduct(id) {\n\n      },\n\n      deleteProduct(id) {\n        this.$store.dispatch(DELETE_PRODUCT)\n      },\n  },\n});\n</script>\n\n\n\n<style scoped>\n  .products {\n    width: 100%;\n    display: flex;\n    justify-content: center;\n  }\n\n  .products-info {\n    width: 90%;\n  }\n\n  .products-columns {\n    width: 100%;\n    font-size: 1.2em;\n    background-color: var(--light-grey);\n    text-transform: uppercase;\n    display: grid;\n    grid-template-columns: 1fr 1fr 1fr;\n    align-items: center;\n    justify-items: center;\n    padding: 10px 0px;\n  }\n\n  .products-columns article {\n    text-align: center;\n  }\n\n  .products-details {\n    cursor: pointer;\n    width: 100%;\n    padding: 30px 0px;\n    text-align: center;\n    display: grid;\n    padding: auto;\n    grid-template-columns: 1fr 1fr 1fr;\n    align-items: center;\n    justify-items: center;\n    border-bottom: 1px solid var(--light-grey);\n  }\n\n  .products-details:hover {\n    background-color: var(--color-background-secondary);\n  }\n\n  .products-details img {\n    width: 50%;\n  }\n\n  .products-details article {\n    text-align: center;\n  }\n\n\n    /* Modal */\n  .modal {\n    display: none;\n    position: fixed;\n    z-index: 1;\n    left: 0;\n    top: 0;\n    width: 100%;\n    height: 100%;\n    overflow: auto;\n    background-color: rgb(0,0,0);\n    background-color: rgba(0,0,0,0.4);\n  }\n\n  .showed {\n    display: block;\n  }\n\n  .modal-content {\n    background-color: #fefefe;\n    margin: 15% auto;\n    padding: 20px;\n    border: 1px solid #888;\n    width: 50%; \n  }\n\n  .close {\n    color: #aaa;\n    float: right;\n    font-size: 28px;\n    font-weight: bold;\n  }\n\n  .close:hover,\n  .close:focus {\n    color: black;\n    text-decoration: none;\n    cursor: pointer;\n  }\n\n  .button {\n    padding: 20px;\n    background-color: white;\n    border: 2px solid black;\n    color: black;\n    font-weight: bold;\n    margin: 20px;\n    cursor: pointer;\n    transition: 0.2s;\n  }\n\n  .button:hover {\n    background-color: var(--color-hover-light);\n  }\n</style>"],"sourceRoot":""}]}